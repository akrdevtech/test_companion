{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "ES2018",
    "lib": [
      "ES2019",
      "ES2020"
    ],
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "files": ["types/request_new_params.d.ts"],
  "include": [
    "lib/**/*",
    "*.ts",
    "types"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}